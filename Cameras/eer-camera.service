[Unit]
Description=Eastern Edge Robotics Camera Module
After=syslog.target
After=network.target

[Service]
ExecStart=/usr/local/bin/mjpg_streamer -i "input_uvc.so -r 640x480 -d /dev/video0 -f 24 -q 80" -o "output_http.so -p 80 -w /usr/local/share/mjpg-streamer/www"
Type=simple
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
